  &lt;span style=&quot;color:#2929cc&quot;&gt;// If z is above the foldingValue we may have to fold. Else early out.&lt;/span&gt;
  &lt;span style=&quot;color:#2e8b57; font-weight:bold&quot;&gt;if&lt;/span&gt; &lt;span style=&quot;color:#676767&quot;&gt;(&lt;/span&gt;side &lt;span style=&quot;color:#676767&quot;&gt;!= -&lt;/span&gt;&lt;span style=&quot;color:#ff1493&quot;&gt;1&lt;/span&gt;&lt;span style=&quot;color:#676767&quot;&gt;)&lt;/span&gt;
