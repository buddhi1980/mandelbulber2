  &lt;span style=&quot;color:#676767&quot;&gt;{&lt;/span&gt;
    b &lt;span style=&quot;color:#676767&quot;&gt;=&lt;/span&gt; solid&lt;span style=&quot;color:#676767&quot;&gt;;&lt;/span&gt;
    c &lt;span style=&quot;color:#676767&quot;&gt;=&lt;/span&gt; L&lt;span style=&quot;color:#676767&quot;&gt;.&lt;/span&gt;&lt;span style=&quot;color:#00008f&quot;&gt;Dot&lt;/span&gt;&lt;span style=&quot;color:#676767&quot;&gt;(&lt;/span&gt;Nv&lt;span style=&quot;color:#676767&quot;&gt;[&lt;/span&gt;i&lt;span style=&quot;color:#676767&quot;&gt;]);&lt;/span&gt;
    &lt;span style=&quot;color:#2929cc&quot;&gt;// A bit subtle here. a_r must be positive and I want to avoid divide by zero.&lt;/span&gt;
    &lt;span style=&quot;color:#2e8b57; font-weight:bold&quot;&gt;if&lt;/span&gt; &lt;span style=&quot;color:#676767&quot;&gt;((&lt;/span&gt;c &lt;span style=&quot;color:#676767&quot;&gt;&amp;gt;&lt;/span&gt; &lt;span style=&quot;color:#ff1493&quot;&gt;0.0&lt;/span&gt;&lt;span style=&quot;color:#676767&quot;&gt;) &amp;amp;&amp;amp; ((&lt;/span&gt;a &lt;span style=&quot;color:#676767&quot;&gt;*&lt;/span&gt; c&lt;span style=&quot;color:#676767&quot;&gt;) &amp;gt;&lt;/span&gt; b&lt;span style=&quot;color:#676767&quot;&gt;))&lt;/span&gt;
    &lt;span style=&quot;color:#676767&quot;&gt;{&lt;/span&gt;
