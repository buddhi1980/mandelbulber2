    CVector3 X_m &lt;span style=&quot;color:#676767&quot;&gt;=&lt;/span&gt; zXYZ &lt;span style=&quot;color:#676767&quot;&gt;-&lt;/span&gt; Nv_m &lt;span style=&quot;color:#676767&quot;&gt;* (&lt;/span&gt;zXYZ&lt;span style=&quot;color:#676767&quot;&gt;.&lt;/span&gt;&lt;span style=&quot;color:#00008f&quot;&gt;Dot&lt;/span&gt;&lt;span style=&quot;color:#676767&quot;&gt;(&lt;/span&gt;Nv_m&lt;span style=&quot;color:#676767&quot;&gt;) -&lt;/span&gt; solid&lt;span style=&quot;color:#676767&quot;&gt;);&lt;/span&gt;

    &lt;span style=&quot;color:#2929cc&quot;&gt;// Find any plane (Nv_r) closest to X_m that cuts the line between Nv_m and X_m.&lt;/span&gt;
    &lt;span style=&quot;color:#2929cc&quot;&gt;// Nv_m cross Nv_r will define a possible rotation axis.&lt;/span&gt;
