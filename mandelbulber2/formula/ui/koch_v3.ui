  &lt;span style=&quot;color:#2e8b57; font-weight:bold&quot;&gt;double&lt;/span&gt; BDscale &lt;span style=&quot;color:#676767&quot;&gt;=&lt;/span&gt; fractal&lt;span style=&quot;color:#676767&quot;&gt;-&amp;gt;&lt;/span&gt;transformCommon&lt;span style=&quot;color:#676767&quot;&gt;.&lt;/span&gt;scale4 &lt;span style=&quot;color:#676767&quot;&gt;*&lt;/span&gt; tc &lt;span style=&quot;color:#676767&quot;&gt;*&lt;/span&gt; tc&lt;span style=&quot;color:#676767&quot;&gt;;&lt;/span&gt;

